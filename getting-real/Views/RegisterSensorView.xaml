<UserControl x:Class="getting_real_4.Views.RegisterSensorView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:getting_real_4.Views"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button Content="Anuller" HorizontalAlignment="Left" Margin="10,272,0,0" VerticalAlignment="Top"
                RenderTransformOrigin="0.289,0.601" />
        <Button Content="Tilføj" HorizontalAlignment="Left" Margin="260,272,0,0" VerticalAlignment="Top"
                RenderTransformOrigin="1.174,0.301" />
        <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Tilføj sensor"
                   VerticalAlignment="Top" />
        <TextBox HorizontalAlignment="Left" Margin="10,92,0,0" TextWrapping="Wrap" Text="{Binding Type}"
                 VerticalAlignment="Top" Width="120" />
        <TextBox HorizontalAlignment="Left" Margin="170,92,0,0" TextWrapping="Wrap" Text="{Binding Keys}"
                 VerticalAlignment="Top" Width="120" />
        <TextBlock HorizontalAlignment="Left" Margin="10,71,0,0" TextWrapping="Wrap" Text="Type"
                   VerticalAlignment="Top" />
        <TextBlock HorizontalAlignment="Left" Margin="170,71,0,0" TextWrapping="Wrap" Text="Nøgler"
                   VerticalAlignment="Top" />
        <TextBox HorizontalAlignment="Left" Margin="10,171,0,0" TextWrapping="Wrap" Text="{Binding SensorType}"
                 VerticalAlignment="Top" Width="120" />
        <TextBlock HorizontalAlignment="Left" Margin="10,150,0,0" TextWrapping="Wrap" Text="Sensor type"
                   VerticalAlignment="Top" />
        <TextBox HorizontalAlignment="Left" Margin="170,171,0,0" TextWrapping="Wrap" Text="{Binding  ConnectionType}"
                 VerticalAlignment="Top" Width="120" />
        <TextBlock HorizontalAlignment="Left" Margin="170,150,0,0" TextWrapping="Wrap" Text="Forbindelsestype"
                   VerticalAlignment="Top" />

    </Grid>
</UserControl>